{
    "name": "aungmyokyaw/zipper",
    "type": "library",
    "description": "This is a little neat helper for the ZipArchive methods with handy functions",
    "keywords": ["laravel", "ZIP", "Archive"],
    "homepage": "http://github.com/Chumper/zipper",
    "license": "Apache2",
    "authors": [
        {
            "name": "Aung Myo Kyaw",
            "email": "aungmyokyaw28637@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2||^8.0",
        "illuminate/support": "^7.0||^8.0||^9.0||^10.0",
        "ext-zip": "*",
        "illuminate/filesystem": "^7.0||^8.0||^9.0||^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Amk\\Zipper\\": "src/Amk/Zipper"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amk\\Zipper\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amk\\Zipper\\ZipperServiceProvider"
            ],
            "aliases": {
                "Zipper": "Amk\\Zipper\\Zipper"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
